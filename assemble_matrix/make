echo "compiling assemblematrix..."

gfortran -fbounds-check -fno-automatic -W -finit-local-zero -std=legacy -o assemblematrix assemblematrix.f

echo ">>>>> gfortran compiler produces erratic output!!!!"
echo ">>>>> ifort -mcmodel=large -shared-intel -o assemblematrix assemblematrix.f"

